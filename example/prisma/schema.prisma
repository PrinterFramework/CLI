generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Sample {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  value1      String
  value2      String
  dateCreated DateTime @default(now())
  dateUpdated DateTime @updatedAt

  @@index(dateCreated)
}
